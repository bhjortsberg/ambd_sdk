include $(MAKE_INCLUDE_GEN)

.PHONY: all clean

#*****************************************************************************#
#                               VARIABLES                               #
#*****************************************************************************#
DIR = $(BASEDIR)/component/common/application/CrashCatcher


#*****************************************************************************#
#                        RULES TO GENERATE TARGETS                            #
#*****************************************************************************#
all:
	@make -C $(DIR)

#*****************************************************************************#
#                        RULES TO CLEAN TARGETS                               #
#*****************************************************************************#
clean:
	@make -C $(DIR) clean

-include $(DEPS)
